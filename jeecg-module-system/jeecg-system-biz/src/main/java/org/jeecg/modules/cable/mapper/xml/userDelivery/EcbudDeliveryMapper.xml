<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="org.jeecg.modules.cable.mapper.dao.userDelivery.EcbudDeliveryDao">

    <!--column-->
    <resultMap id="BaseResultMap" type="org.jeecg.modules.cable.entity.userDelivery.EcbudDelivery">
        <id column="ecbudd_id" jdbcType="INTEGER" property="ecbuddId"/>
        <result column="ec_company_id" jdbcType="INTEGER" property="ecCompanyId"/>
        <result column="ecu_id" jdbcType="INTEGER" property="ecuId"/>
        <result column="sort_id" jdbcType="INTEGER" property="sortId"/>
    </resultMap>

    <sql id="Base_Column_List">
        ecbudd_id
        , ec_company_id, ecu_id, sort_id
    </sql>

    <!-- getObject -->
    <select id="getObject" parameterType="org.jeecg.modules.cable.entity.userDelivery.EcbudDelivery" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from ecbud_delivery
        <where>
            <if test="ecCompanyId != null">
                and ec_company_id = #{ecCompanyId,jdbcType=INTEGER}
            </if>
            <if test="ecuId != null">
                and ecu_id = #{ecuId,jdbcType=INTEGER}
            </if>
        </where>
        limit 1
    </select>


    <!--insert-->
    <insert id="insert" parameterType="org.jeecg.modules.cable.entity.userDelivery.EcbudDelivery">
        insert into ecbud_delivery (ecbudd_id, ec_company_id, ecu_id,
                                    sort_id)
        values (#{ecbuddId,jdbcType=INTEGER}, #{ecCompanyId,jdbcType=INTEGER}, #{ecuId,jdbcType=INTEGER},
                #{sortId,jdbcType=INTEGER})
    </insert>

    <update id="updateByPrimaryKeySelective" parameterType="org.jeecg.modules.cable.entity.userDelivery.EcbudDelivery">
        update ecbud_delivery
        <set>
            <if test="ecCompanyId != null">
                ec_company_id = #{ecCompanyId,jdbcType=INTEGER},
            </if>
            <if test="ecuId != null">
                ecu_id = #{ecuId,jdbcType=INTEGER},
            </if>
            <if test="sortId != null">
                sort_id = #{sortId,jdbcType=INTEGER},
            </if>
        </set>
        where ecbudd_id = #{ecbuddId,jdbcType=INTEGER}
    </update>


</mapper>


