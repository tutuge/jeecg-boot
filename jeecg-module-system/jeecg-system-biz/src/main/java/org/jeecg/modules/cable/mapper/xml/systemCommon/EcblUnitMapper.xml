        select
        <include refid="Base_Column_List" />
        from ecbl_unit
        <where>
            <if test="ecbluId != null">
                <if test="ecbluId != 0 and lengthName != null">
                    and <![CDATA[ecblu_id != #{ecbluId,jdbcType=INTEGER}]]>
                </if>
                <if test="lengthName == null">
                    and ecblu_id = #{ecbluId,jdbcType=INTEGER}
