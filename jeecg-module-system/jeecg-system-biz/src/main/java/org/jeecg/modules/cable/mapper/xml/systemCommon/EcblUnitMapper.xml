        values (#{ecbluId,jdbcType=INTEGER}, #{startType,jdbcType=BIT},
                #{sortId,jdbcType=INTEGER}, #{lengthName,jdbcType=VARCHAR}, #{meterNumber,jdbcType=INTEGER},
                #{description,jdbcType=VARCHAR})
