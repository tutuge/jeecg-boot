    <!-- getList -->
    <select id="getList" parameterType="org.jeecg.modules.cable.entity.systemCommon.EcblUnit" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from ecbl_unit
        <where>
            <if test="startType!=null">
                and start_type=#{startType}
