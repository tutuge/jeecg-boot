<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="org.jeecg.modules.cable.mapper.dao.userEcable.EcbuSheathMapper">

    <!--column-->
    <resultMap id="BaseResultMap" type="org.jeecg.modules.cable.entity.userEcable.EcbuSheath">
        <id column="ecbus_id" jdbcType="INTEGER" property="ecbusId"/>
        <result column="ecbs_id" jdbcType="INTEGER" property="ecbsId"/>
        <result column="ec_company_id" jdbcType="INTEGER" property="ecCompanyId"/>
        <result column="start_type" jdbcType="BIT" property="startType"/>
        <result column="name" jdbcType="VARCHAR" property="name"/>
        <result column="unit_price" jdbcType="DECIMAL" property="unitPrice"/>
        <result column="density" jdbcType="DECIMAL" property="density"/>
        <result column="description" jdbcType="VARCHAR" property="description"/>
    </resultMap>

    <resultMap id="UnionResultMap" type="org.jeecg.modules.cable.entity.userEcable.EcbuSheath">
        <id column="ecbus_id" jdbcType="INTEGER" property="ecbusId"/>
        <result column="ecbs_id" jdbcType="INTEGER" property="ecbsId"/>
        <result column="ec_company_id" jdbcType="INTEGER" property="ecCompanyId"/>
        <result column="start_type" jdbcType="BIT" property="startType"/>
        <result column="name" jdbcType="VARCHAR" property="name"/>
        <result column="unit_price" jdbcType="DECIMAL" property="unitPrice"/>
        <result column="density" jdbcType="DECIMAL" property="density"/>
        <result column="description" jdbcType="VARCHAR" property="description"/>
        <association property="ecbSheath" javaType="org.jeecg.modules.cable.entity.systemEcable.EcbSheath">
            <result column="abbreviation" property="abbreviation"/>
        </association>
    </resultMap>

    <sql id="Base_Column_List">
        ecbus_id
        , ecbs_id, ec_company_id, start_type, name, unit_price, density, description
    </sql>

    <!-- With_List -->
    <sql id="With_List">
        ecbus
        .
        ecbus_id
        , ecbus.ecbs_id, ecbus.ec_company_id, ecbus.start_type, ecbus.name, ecbus.unit_price, ecbus.density,
   	ecbus.description,
   	ecbs.abbreviation
    </sql>
    <!-- getList -->
    <select id="getList" parameterType="org.jeecg.modules.cable.entity.userEcable.EcbuSheath" resultMap="UnionResultMap">
        select
        <include refid="With_List"/>
        from ecbu_sheath as ecbus
        left join ecb_sheath as ecbs on ecbus.ecbs_id=ecbs.ecbs_id
        <where>
            <if test="startType!=null">
                AND ecbus.start_type=#{startType}
            </if>
            <if test="ecCompanyId!=null">
                AND ecbus.ec_company_id=#{ecCompanyId}
            </if>
            <if test="ecbsId!=null">
                AND ecbus.ecbs_id=#{ecbsId}
            </if>
        </where>
        order by ecbs.sort_id asc
    </select>

    <!--getObject-->
    <select id="getObject" parameterType="org.jeecg.modules.cable.entity.userEcable.EcbuSheath" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from ecbu_sheath
        <where>
            <if test="ecbusId != null">
                and ecbus_id = #{ecbusId,jdbcType=INTEGER}
            </if>
            <if test="ecbsId != null">
                and ecbs_id = #{ecbsId,jdbcType=INTEGER}
            </if>
            <if test="ecCompanyId != null">
                and ec_company_id = #{ecCompanyId,jdbcType=INTEGER}
            </if>
        </where>
        limit 1
    </select>

    <!-- delete -->
    <delete id="deleteByCompanyId" parameterType="org.jeecg.modules.cable.entity.userEcable.EcbuSheath">
        delete from ecbu_sheath
        <where>
            <if test="ecCompanyId != null">
                and ec_company_id = #{ecCompanyId,jdbcType=INTEGER}
            </if>
        </where>
    </delete>


</mapper>


